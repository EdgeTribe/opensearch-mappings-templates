{
    "template": {
        "mappings": {
            "_meta": {
                "schema_version": 1
            },
            "dynamic_templates": [
                {
                    "strings": {
                        "match_mapping_type": "string",
                        "mapping": {
                            "type": "keyword"
                        }
                    }
                }
            ],
            "properties": {
                "@timestamp": {
                    "type": "date"
                },
                "destination": {
                    "properties": {
                        "address": {
                            "type": "ip",
                            "fields": {
                                "keyword": {
                                    "type": "keyword"
                                }
                            }
                        },
                        "port": {
                            "type": "long"
                        },
                        
                    }
                }
            }
        }
    }
}